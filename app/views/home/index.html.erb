<div>
  <div class="col-12 my-3">
    <p class="<%= 'bg-transparent border-0 m-0' unless notice || alert %> notice alert-warning alert-dismissible fade show"><%= notice %></p>
    <p class="<%= 'bg-transparent border-0 m-0' unless notice || alert %> alert alert-danger alert-dismissible fade show" role='alert'><%= alert %></p>
  </div>
  <%= link_to "Dashboard", dashboard_path %>
  <h2>Here are your current arrows</h2>
  <% unless @arrows.blank? %>
    <%= render @arrows %>
  <% end %>
</div>

